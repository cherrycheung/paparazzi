<!DOCTYPE module SYSTEM "module.dtd">

<module name="gps_avoid">
  <doc>
    <description>This module is generates a command to avoid other vehicles based on their relative gps location</description>
  </doc>
  <header>
    <file name="gps_avoid.h"/>
  </header>
  <init fun="gps_avoid_init()"/>
  <periodic fun="gps_avoid_periodic()" freq="2" autorun="TRUE"/>
  <datalink message="REMOTE_GPS_SMALL" fun="gps_avoid_parse_REMOTE_GPS_SMALL()"/>
  <makefile>
    <file name="gps_avoid.c"/>
  </makefile>
</module>

