<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="1.5" ground_alt="0" lat0="51.9906322" lon0="4.3767944" max_dist_from_home="15" name="Rotorcraft Basic (Enac)" security_height="0.5">
  <header>
#include "autopilot.h"

</header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="0.0" y="0.0"/>
    <waypoint name="STDBY" x="0.0" y="0.0"/>
    <waypoint name="TD" x="0.0" y="0.0"/>
    <waypoint lat="51.990610" lon="4.376794" name="p1"/>
    <waypoint lat="51.990621" lon="4.376776" name="p2"/>
    <waypoint lat="51.990643" lon="4.376813" name="p3"/>
    <waypoint lat="51.990654" lon="4.376794" name="p4"/>
    <waypoint lat="51.990643" lon="4.376777" name="p5"/>
    <waypoint lat="51.990621" lon="4.376812" name="p6"/>
    <waypoint lat="51.990646" lon="4.376724" name="FA1"/>
    <waypoint lat="51.990677" lon="4.376812" name="FA2"/>
    <waypoint lat="51.990624" lon="4.376867" name="FA3"/>
    <waypoint lat="51.990593" lon="4.376779" name="FA4"/>
    <waypoint lat="51.9906253" lon="4.3768574" name="FZ3"/>
    <waypoint lat="51.9905993" lon="4.3767808" name="FZ4"/>
    <waypoint lat="51.9906445" lon="4.3767337" name="FZ1"/>
    <waypoint lat="51.9906707" lon="4.3768103" name="FZ2"/>
  </waypoints>
  <sectors>
    <sector color="red" name="Flight_Area">
      <corner name="FA4"/>
      <corner name="FA3"/>
      <corner name="FA2"/>
      <corner name="FA1"/>
    </sector>
    <sector color="blue" name="Flight_Zone">
      <corner name="FZ2"/>
      <corner name="FZ3"/>
      <corner name="FZ4"/>
      <corner name="FZ1"/>
    </sector>
  </sectors>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetWaypointHere(WP_HOME)"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <call fun="NavSetWaypointHere(WP_STDBY)"/>
      <call fun="NavSetWaypointHere(WP_TD)"/>
      <call fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Start Engine">
      <call fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="stateGetPositionEnu_f()->z > 2.0" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="0.5" vmode="climb" wp="CLIMB"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
    <block name="prepre1">
      <call fun="nav_set_heading_deg(30)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre2"/>
    </block>
    <block name="prepre2">
      <call fun="nav_set_heading_deg(60)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre3"/>
    </block>
    <block name="prepre3">
      <call fun="nav_set_heading_deg(90)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre4"/>
    </block>
    <block name="prepre4">
      <call fun="nav_set_heading_deg(120)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre5"/>
    </block>
    <block name="prepre5">
      <call fun="nav_set_heading_deg(150)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre6"/>
    </block>
    <block name="prepre6">
      <call fun="nav_set_heading_deg(180)"/>
      <stay wp="STDBY" until="stage_time>2"/>
      <deroute block="prepre7"/>
    </block>
    <block name="prepre7">
      <stay wp="p1" until="stage_time>5"/>
      <deroute block="prepre8"/>
    </block>
    <block name="prepre8">
      <call fun="nav_set_heading_deg(-100)"/>
      <stay wp="p1" until="stage_time>2"/>
      <deroute block="1 to 2"/>
    </block>
    <block name="1 to 2" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p2)"/>
      <stay wp="p2" until="stage_time>10"/>
      <exception cond="valueofdetection == 1" deroute="avoid 1 to 2"/>
      <deroute block="2 to 3"/>
    </block>
    <block name="2 to 3" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p3)"/>
      <stay wp="p3" until="stage_time>10"/>
      <exception cond="valueofdetection == 1" deroute="avoid 2 to 3"/>
      <deroute block="3 to 4"/>
    </block>
    <block name="3 to 4" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p4)"/>
      <stay wp="p4" until="stage_time>10"/>
      <exception cond="valueofdetection == 1" deroute="avoid 3 to 4"/>
      <deroute block="4 to 5"/>
    </block>
    <block name="4 to 5" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p5)"/>
      <stay wp="p5" until="stage_time>10"/>
      <exception cond="valueofdetection == 1" deroute="avoid 4 to 5"/>
      <deroute block="5 to 6"/>
    </block>
    <block name="5 to 6" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p6)"/>
      <stay wp="p6" until="stage_time>15"/>
      <exception cond="valueofdetection == 1" deroute="avoid 5 to 6"/>
      <deroute block="6 to 1"/>
    </block>
    <block name="6 to 1" pre_call="avoid_detection1()">
      <call fun="nav_set_heading_towards_waypoint(WP_p1)"/>
      <stay wp="p6" until="stage_time>10"/>
      <exception cond="valueofdetection == 1" deroute="avoid 6 to 1"/>
      <deroute block="prepre7"/>
    </block>
<!-- NAVIGATION BLOCKS-->
    <block name="avoid 1 to 2">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p2)"/>
      <call fun="safe_warning()"/>
      <stay wp="p2" until="stage_time>10"/>
      <deroute block="2 to 3"/>
    </block>
    <block name="avoid 2 to 3">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p3)"/>
      <call fun="safe_warning()"/>
      <stay wp="p3" until="stage_time>15"/>
      <deroute block="3 to 4"/>
    </block>
    <block name="avoid 3 to 4">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p4)"/>
      <call fun="safe_warning()"/>
      <stay wp="p4" until="stage_time>10"/>
      <deroute block="4 to 5"/>
    </block>
    <block name="avoid 4 to 5">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p5)"/>
      <call fun="safe_warning()"/>
      <stay wp="p5" until="stage_time>10"/>
      <deroute block="5 to 6"/>
    </block>
    <block name="avoid 5 to 6">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p6)"/>
      <call fun="safe_warning()"/>
      <stay wp="p6" until="stage_time>10"/>
      <deroute block="6 to 1"/>
    </block>
    <block name="avoid 6 to 1">
      <circle wp="HOME" radius="-0.8" until="stage_time>7"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p1)"/>
      <call fun="safe_warning()"/>
      <stay wp="p1" until="stage_time>10"/>
      <deroute block="1 to 2"/>
    </block>

    <block name="test">
      <call fun="avoid_navigation1()"/>
    </block>

    <!--block name="stay_p1">
      <stay until="stage_time>15" wp="p1"/>
      <deroute block="go from 1 to 2"/>
    </block>
    <block name="go from 1 to 2">
      <call fun="nav_set_heading_towards_waypoint(WP_p2)"/>
      <deroute block="stay_p2"/>
    </block>
    <block name="stay_p2">
      <stay until="stage_time>15" wp="p2"/>
      <deroute block="go from 2 to 3"/>
    </block>
    <block name="go from 2 to 3">
      <call fun="nav_set_heading_towards_waypoint(WP_p3)"/>
    </block>
    <block name="stay_p3">
      <stay until="stage_time>15" wp="p3"/>
    </block>
    <block name="go from 3 to 4">
      <call fun="nav_set_heading_towards_waypoint(WP_p4)"/>
    </block>
    <block name="stay_p4">
      <stay until="stage_time>15" wp="p4"/>
    </block>
    <block name="go from 4 to 5">
      <call fun="nav_set_heading_towards_waypoint(WP_p5)"/>
      <deroute block="stay_p5"/>
    </block>
    <block name="stay_p5">
      <stay until="stage_time>15" wp="p5"/>
    </block>
    <block name="go from 5 to 6">
      <call fun="nav_set_heading_towards_waypoint(WP_p6)"/>
    </block>
    <block name="stay_p6">
      <stay until="stage_time>15" wp="p6"/>
      <call fun="nav_set_heading_towards_waypoint(WP_p1)" until="stage_time>18"/>
      <deroute block="stay_p1"/>
    </block-->
    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="-0.8" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
